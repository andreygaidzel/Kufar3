<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css"/>
    <link href="~/Content/scroll.css" rel="stylesheet"/>
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <link href="~/Content/SiteDeclaration.css" rel="stylesheet"/>
</head>
<body>
@Html.Action("LayoutHeader", "Home")
<div class="container">
    <div class="menu">
        @*@Html.Partial("~/Views/Home/_CatalogMenu.cshtml")*@
        @Html.Action("MenuLeft", "Home")
    </div>
    <div class="content">
        <div class="content-holder">

            @RenderBody()
        </div>
    </div>
</div>
<script src="~/MyScripts/UploadImages.js"></script>
</body>
</html>